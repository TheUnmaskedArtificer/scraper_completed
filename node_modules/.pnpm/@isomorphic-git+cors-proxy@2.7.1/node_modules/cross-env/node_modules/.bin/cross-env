#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/bin/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/bin/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/cross-env@5.2.1/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/cross-env.js" "$@"
else
  exec node  "$basedir/../../dist/bin/cross-env.js" "$@"
fi
