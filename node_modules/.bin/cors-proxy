#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/src/scraper_completed/node_modules/.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules/@isomorphic-git/cors-proxy/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules/@isomorphic-git/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/src/scraper_completed/node_modules/.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules/@isomorphic-git/cors-proxy/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules/@isomorphic-git/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules:/mnt/c/src/scraper_completed/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules/@isomorphic-git/cors-proxy/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/@isomorphic-git+cors-proxy@2.7.1/node_modules/@isomorphic-git/cors-proxy/bin.js" "$@"
fi
